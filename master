button6.MouseButton1Down:connect(function()
	game.StarterGui:SetCore("SendNotification", {
                Title = "Anti-Ban";
                Text = "You will turn invisible when a mod/admin joins.";
                 Duration = 15;
	              })
	
	local AdminList = {
21238127, 
24836348, 
23952793, 
6828351, 
109217153, 
119868, 
39523020, 
14922769, 
35276317, 
