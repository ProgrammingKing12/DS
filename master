local function bro()
    local script1 = Instance.new('LocalScript', text1)
	local script2 = Instance.new('LocalScript', FunFrame)
	local script3 = Instance.new('LocalScript', DNAFarmFrame)
	local script4 = Instance.new('LocalScript', ElderDinoFrame)
	local script5 = Instance.new('LocalScript', ExclusiveFrame)
	local script6 = Instance.new('LocalScript', LocalPlayerFrame)
	local script7 = Instance.new('LocalScript', MapsFrame)
	local script8 = Instance.new('LocalScript', borderss)
	local script9 = Instance.new('LocalScript', text2)
	local script10 = Instance.new('LocalScript', MAINText)
	local script11 = Instance.new('LocalScript', MISCText)
	local script12 = Instance.new('LocalScript', LocalPlayer)
	local script13 = Instance.new('LocalScript', Players)
	local script14 = Instance.new('LocalScript', World)
	local script15 = Instance.new('LocalScript', Maps)
	local script16 = Instance.new('LocalScript', Elder)
	local script17 = Instance.new('LocalScript', Trading)
	local script18 = Instance.new('LocalScript', localptext)
	local script19 = Instance.new('LocalScript', localptext2)
	local script20 = Instance.new('LocalScript', textspecial)
	local script21 = Instance.new('LocalScript', textspecial2)
	local script22 = Instance.new('LocalScript', Textfun2)
	local script23 = Instance.new('LocalScript', Textfun)
	local script24 = Instance.new('LocalScript', Mapstext)
	local script25 = Instance.new('LocalScript', eldertext2)
	local script26 = Instance.new('LocalScript', eldertext1)
	local script27 = Instance.new('LocalScript', TextDNAfarm)
	local script28 = Instance.new('LocalScript', borderss1)
	local script29 = Instance.new('LocalScript', borderss2)
	local script30 = Instance.new('LocalScript', borderss3)
	local script31 = Instance.new('LocalScript', borderss4)
	local script32 = Instance.new('LocalScript', borderss5)
	local script33 = Instance.new('LocalScript', borderss6)
	local script34 = Instance.new('LocalScript', borderss7)
	local script35 = Instance.new('LocalScript', borderss8)



	function zigzag(X)return math.acos(math.cos(X*math.pi))/math.pi end
	counter=0
	
	while wait(0.1)do
	    script1.Parent.BackgroundColor3=Color3.fromHSV(zigzag(counter),1,1)
		counter = counter + 0.001
		script2.Parent.BorderColor3=Color3.fromHSV(zigzag(counter),1,1)
		counter = counter + 0.001
		script3.Parent.BorderColor3=Color3.fromHSV(zigzag(counter),1,1)
		counter = counter + 0.001
		script4.Parent.BorderColor3=Color3.fromHSV(zigzag(counter),1,1)
		counter = counter + 0.001
		script5.Parent.BorderColor3=Color3.fromHSV(zigzag(counter),1,1)
		counter = counter + 0.001
		script6.Parent.BorderColor3=Color3.fromHSV(zigzag(counter),1,1)
		counter = counter + 0.001
		script7.Parent.BorderColor3=Color3.fromHSV(zigzag(counter),1,1)
		counter = counter + 0.001
		script8.Parent.BackgroundColor3=Color3.fromHSV(zigzag(counter),1,1)
		script9.Parent.TextColor3=Color3.fromHSV(zigzag(counter),1,1)
		script10.Parent.TextColor3=Color3.fromHSV(zigzag(counter),1,1)
		script11.Parent.TextColor3=Color3.fromHSV(zigzag(counter),1,1)
		script12.Parent.TextColor3=Color3.fromHSV(zigzag(counter),1,1)
		script13.Parent.TextColor3=Color3.fromHSV(zigzag(counter),1,1)
		script14.Parent.TextColor3=Color3.fromHSV(zigzag(counter),1,1)
		script15.Parent.TextColor3=Color3.fromHSV(zigzag(counter),1,1)
		script16.Parent.TextColor3=Color3.fromHSV(zigzag(counter),1,1)
		script17.Parent.TextColor3=Color3.fromHSV(zigzag(counter),1,1)
		script18.Parent.TextColor3=Color3.fromHSV(zigzag(counter),1,1)
		script19.Parent.TextColor3=Color3.fromHSV(zigzag(counter),1,1)
		script20.Parent.TextColor3=Color3.fromHSV(zigzag(counter),1,1)
		script21.Parent.TextColor3=Color3.fromHSV(zigzag(counter),1,1)
		script22.Parent.TextColor3=Color3.fromHSV(zigzag(counter),1,1)
		script23.Parent.TextColor3=Color3.fromHSV(zigzag(counter),1,1)
		script24.Parent.TextColor3=Color3.fromHSV(zigzag(counter),1,1)
		script25.Parent.TextColor3=Color3.fromHSV(zigzag(counter),1,1)
		script26.Parent.TextColor3=Color3.fromHSV(zigzag(counter),1,1)
		script27.Parent.TextColor3=Color3.fromHSV(zigzag(counter),1,1)
		script28.Parent.BackgroundColor3=Color3.fromHSV(zigzag(counter),1,1)
		script29.Parent.BackgroundColor3=Color3.fromHSV(zigzag(counter),1,1)
		script30.Parent.BackgroundColor3=Color3.fromHSV(zigzag(counter),1,1)
		script31.Parent.BackgroundColor3=Color3.fromHSV(zigzag(counter),1,1)
		script32.Parent.BackgroundColor3=Color3.fromHSV(zigzag(counter),1,1)
		script33.Parent.BackgroundColor3=Color3.fromHSV(zigzag(counter),1,1)
		script34.Parent.BackgroundColor3=Color3.fromHSV(zigzag(counter),1,1)
		script35.Parent.BackgroundColor3=Color3.fromHSV(zigzag(counter),1,1)
	end
end

coroutine.wrap(bro)()

