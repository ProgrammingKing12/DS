	Terrain = true;
	ColorCorrection = true;
	Sun = true;
	Lighting = true;
	BloomEffect = true;
	
}

-- settings {

color.Enabled = true
color.Contrast = 0.15
color.Brightness = 0.074
color.Saturation = 0.25
color.TintColor = Color3.fromRGB(248, 248, 248)

sun.Enabled = true
sun.Intensity = 0.2
sun.Spread = 1

bloom.Enabled = true
bloom.Intensity = 0.25
bloom.Size = 15
bloom.Threshold = 0.5

blur.Enabled = true
blur.Size = 0

-- settings }


